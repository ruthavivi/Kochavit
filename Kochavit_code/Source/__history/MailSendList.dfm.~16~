  object qrEMailsSendCl: TEDBQuery
    DatabaseName = 'dbnMain'
    SessionName = 'Main'
    DataSource = dmClientEdit.dsClient
    SQL.Strings = (
      'SELECT *'
      'FROM KEmailsSend Es'
      'WHERE ClientId = :Id  and CarId IS NULL and DriverId IS NULL'
      'ORDER BY Moed DESC')
    Params = <
