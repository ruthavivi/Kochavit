  inherited qrFilter: TEDBQuery
    SQL.Strings = (
      
        'SELECT Dc.StartD, Dc.EndD, Dc.Organize, Cl.Shem, Dr.ShemD, tC.Co' +
        'urse'
      'FROM KDrCourse.db Dc'
      '   LEFT JOIN KDriver.db Dr ON Dc.IdDriver = Dr.Id'
      '         LEFT JOIN KClient.db Cl ON Dr.ClientId = Cl.Id'
      '   LEFT JOIN KTblCourse.db tC ON Dc.IdCourse = tC.Id'
      'WHERE 0=0'
      'ORDER BY Dc.StartD')
    object qrFilterStartD: TDateField
      FieldName = 'StartD'
    end
    object qrFilterEndD: TDateField
      FieldName = 'EndD'
    end
    object qrFilterOrganize: TStringField
      FieldName = 'Organize'
    end
    object qrFilterShem: TStringField
      FieldName = 'Shem'
      Size = 30
    end
    object qrFilterShemD: TStringField
      FieldName = 'ShemD'
      Size = 25
    end
    object qrFilterCourse: TStringField
      FieldName = 'Course'
      Size = 25
    end
  end
end
