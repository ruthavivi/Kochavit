inherited dmClientFilter: TdmClientFilter
  inherited qrFilter: TEDBQuery
    SQL.Strings = (
      'SELECT Cl.*, Oved, OfenPay'
      'FROM KClient Cl        '
      'LEFT JOIN KTblOved tV ON Cl.OvedId = tV.Id'
      'LEFT JOIN KTblOfenPay tP ON Cl.OfenPayId = tP.Id')
    ReadOnly = True
