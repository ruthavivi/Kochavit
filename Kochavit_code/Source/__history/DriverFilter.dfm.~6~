  inherited qrFilter: TEDBQuery
    SQL.Strings = (
      'SELECT Dr.*, Cl.Shem, Cl.Hp, Cl.IdClient'
      'FROM KDriver Dr'
      'LEFT JOIN KClient Cl ON (Dr.ClientId = Cl.Id)'
      'WHERE 0=0'
      'ORDER BY Dr.ShemD')
    Top = 112
