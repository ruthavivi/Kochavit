  inherited qrFilter: TEDBQuery
    SQL.Strings = (
      'SELECT Da.Id, Da.AcdntD, Da.WithInjr, Da.Quilty, Da.Tipul, '
      'Cr.Number, Cl.Shem, Dr.ShemD'
      'FROM KDrAcdnt Da'
      '   LEFT JOIN KCar Cr ON Da.IdCar = Cr.Id'
      '   LEFT JOIN KDriver Dr ON Da.IdDriver = Dr.Id'
      '         LEFT JOIN KClient Cl ON Dr.ClientId = Cl.Id'
      'WHERE 0=0'
      'ORDER BY Da.AcdntD')
  end
