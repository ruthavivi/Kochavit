  object qrDtl: TEDBQuery
    DatabaseName = 'dbnMain'
    SessionName = 'Main'
    DataSource = dsFilter
    SQL.Strings = (
      'SELECT BikoretDtl, TipulId'
      'FROM KCrTipulDtlHis Td'
      'LEFT JOIN KTblBikoretDtl tB ON Td.BikoretDtlId = tB.Id'
