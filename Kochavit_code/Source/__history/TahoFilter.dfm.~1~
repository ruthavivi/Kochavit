  inherited tbl: TEDBTable
    Left = 80
    Top = 96
  end
  inherited qrFilter: TEDBQuery
    SQL.Strings = (
      
        'SELECT Dt.Id, Dt.Moed, Dt.Speed, Dt.Zman, Dt.Harig, Dt.Remark, C' +
        'r.Number, Dr.ShemD, Cl.Shem'
      'FROM KDrTaho Dt'
      '   LEFT JOIN KCar Cr ON Dt.CarId = Cr.Id'
      '         LEFT JOIN KClient Cl ON Cr.ClientId = Cl.Id'
      '   LEFT JOIN KDriver Dr ON Dt.DrvId = Dr.Id'
      'WHERE 0=0'
      'ORDER BY Dt.Moed')
    object qrFilterId: TIntegerField
      FieldName = 'Id'
    end
    object qrFilterMoed: TDateField
      FieldName = 'Moed'
    end
    object qrFilterSpeed: TBooleanField
      FieldName = 'Speed'
      OnGetText = qrFilterSpeedGetText
    end
    object qrFilterZman: TBooleanField
      FieldName = 'Zman'
      OnGetText = qrFilterSpeedGetText
    end
    object qrFilterHarig: TBooleanField
      FieldName = 'Harig'
      OnGetText = qrFilterSpeedGetText
    end
    object qrFilterRemark: TStringField
      FieldName = 'Remark'
      Size = 30
    end
    object qrFilterNumber: TStringField
      FieldName = 'Number'
      Size = 9
    end
    object qrFilterShemD: TStringField
      FieldName = 'ShemD'
      Size = 25
    end
    object qrFilterShem: TStringField
      FieldName = 'Shem'
      Size = 30
    end
  end
