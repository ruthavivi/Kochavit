  inherited qrFilter: TQuery
    SQL.Strings = (
      'SELECT Ht.*, Dr.ShemD, Cl.Shem, Th.Heiter'
      'FROM KDrHeiterHis Ht'
      'LEFT JOIN KDriver Dr ON Dr.Id = Ht.IdDriver'
      'LEFT JOIN KClient Cl ON Cl.Id = Dr.ClientId'
      'LEFT JOIN KTblHeiter Th ON Th.Id = Ht.IdHeiter'
      'WHERE 0=0'
      'ORDER BY Ht.DateDone')
    Left = 32
    Top = 80
    object qrFilterId: TIntegerField
      FieldName = 'Id'
    end
