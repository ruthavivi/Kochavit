  inherited qrFilter: TEDBQuery
    SQL.Strings = (
      
        'SELECT CAST(Bl.ReciptNo AS NUMERIC) ReciptNo, Bl.Id, Bl.ClientId' +
        ', Bl.ProduceD, Bl.SumA,Bl.ToPayD, Bl.APeriod, Bl.ZPeriod, Bl.Pay' +
        'dD, Bl.Remark, Cl.Shem, (Bl.PaydD-Bl.ToPayD) Delta, Bl.Kabala'
      'FROM KBillHis Bl'
      'LEFT JOIN KClient Cl ON Cl.Id = Bl.ClientId'
      'WHERE 0=0'
      'ORDER BY Bl.ReciptNo')
    Left = 72
    Top = 72
